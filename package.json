{
    "name": "stackerjs-utils",
    "version": "1.1.3",
    "keywords": [
        "stackerjs",
        "utilities",
        "framework"
    ],
    "author": {
        "name": "Vinicius Guedes",
        "email": "viniciusgued@gmail.com",
        "url": "https://vinyguedess.github.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/parpeoficial/stackerjs-utils"
    },
    "main": "lib/index.js",
    "types": "index.d.ts",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "global": "^4.3.2",
        "semantic-release-cli": "^4.0.1",
